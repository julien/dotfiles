nonblock on
bindkey "^[[1;5C" next
bindkey "^[[1;5D" prev
bind n screen
bind w windowlist
bind x kill
bindkey -m y eval 'stuff Y' 'writebuf' 'exec sh -c "/usr/bin/pbcopy < /tmp/screen-exchange"'
:bufferfile
